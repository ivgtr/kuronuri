{
  "name": "kuronuri",
  "version": "1.3.0",
  "description": "ðŸ¤¦ Nouns that lead to personal identification can be blacked out.",
  "keywords": [
    "black-out",
    "cli",
    "kuronuri",
    "noun-hide"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "kuronuri": "./dist/cli.js"
  },
  "engines": {
    "node": ">= 12.2.0"
  },
  "license": "MIT",
  "author": {
    "name": "ivgtr",
    "email": "ivgtr.me@gmail.com",
    "url": "https://github.com/ivgtr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivgtr/kuronuri.git"
  },
  "homepage": "https://github.com/ivgtr/kuronuri#readme",
  "bugs": {
    "url": "https://github.com/ivgtr/kuronuri/issues"
  },
  "files": [
    "dist",
    "src/index.ts",
    "src/cli.ts"
  ],
  "scripts": {
    "dev": "node --loader ts-node/esm ./src/cli.ts",
    "start": "tsc -w",
    "type-check": "tsc --noEmit",
    "type-build": "tsc --project ./tsconfig-build.json --emitDeclarationOnly",
    "prebuild": "node -e 'fs.rmSync(`dist`, {recursive:true, force:true})'",
    "build": "rollup -c && npm run type-build",
    "format": "prettier -w ./src/*",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@babel/preset-env": "7.16.8",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@rollup/plugin-typescript": "8.3.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@types/update-notifier": "5.1.0",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "rollup": "2.63.0",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-preserve-shebang": "1.0.1",
    "rollup-plugin-terser": "7.0.2",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "kuromojin": "^3.0.0",
    "meow": "^10.1.2",
    "update-notifier": "^5.1.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "preset": "ts-jest/presets/js-with-ts-esm",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
